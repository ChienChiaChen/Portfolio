<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="password_edit_text">
        <attr name="pet_iconShow" format="integer"/>
        <attr name="pet_iconHide" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RoundImageViewByXfermode">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />

    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="TintableImageView">
        <attr name="pfw_tint" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="TrackItem">
        <attr name="track_item_index" format="string" />
        <attr name="track_item_title" format="string" />
        <attr name="track_item_subtitle" format="string" />
    </declare-styleable>

</resources>